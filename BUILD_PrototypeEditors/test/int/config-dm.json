{
    "env": {
        "NODE_ENV": "development"
    },
    "debug": {
        "active": false,
        "liveReload": [
            "/norman"
        ]
    },
    "server": {},
    "logging": {
        "logDirectory": "logs",
        "output": {
            "stdout": {
                "type": "console"
            },
            "logfile": {
                "type": "file",
                "path": "norman_{now:yyyyMMdd}.log"
            }
        },
        "loggers": {
            "*": {
                "stdout": "info",
                "logfile": "info"
            }
        }
    },
    "db": {
        "hosts": "localhost",
        "database": "norman-prototype-editors-test-datamodeler",
        "options": {
            "db": {
                "safe": true
            },
            "server": {
                "poolSize": 20,
                "socketOptions": {
                    "keepAlive": 1
                }
            }
        }
    },
    "deployment": {
        "strategy": "single"
    },
    "http": {
        "port": 9029,
        "secureFlag": false
    },
    "web": {
        "root": "../dev",
        "errors": {
            "root": "errors"
        },
        "indexFallback": [
            "/verifyemail",
            "/auth",
            "/login",
            "/signup",
            "/norman"
        ],
        "compression": {
            "threshold": 512
        }
    },
    "features": "./config/features.json",
    "security": "./config/security.json",
    "access": "./config/access.json",
    "account": "./config/account.json",
    "services": "./config/services.json",
    "session": {
        "secret": "711606be5b72f394ba28758351779906af717e3cdf5a761c4ce9f01356b512b4"
    }
}
